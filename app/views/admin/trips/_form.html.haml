= form_for [:admin, @trip] do |f|
  = f.label :subscription_type, 'Subscription Type'
  -# = f.number_field :subscription_type
  = f.select(:subscription_type) do
    -[['Subscriber','subscriber'],['Customer','customer']].each do |c|
      =content_tag(:option, c.first, value: c.last)

  = f.label :duration
  = f.number_field :duration

  = f.label :zip_code
  = f.number_field :zip_code

  = f.label :bike_id
  = f.number_field :bike_id

  = f.label :start_station_id
  = f.number_field :start_station_id

  = f.label :end_station_id
  = f.number_field :end_station_id

  = f.label :start_date
  = f.datetime_local_field :start_date

  = f.label :end_date
  = f.datetime_local_field :end_date

  = f.submit
